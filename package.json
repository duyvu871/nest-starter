{
  "name": "basic-pos-system",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && tsc-alias -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development && nest start --watch",
    "start:debug": "cross-env NODE_ENV=development && nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test && jest",
    "test:watch": "cross-env NODE_ENV=test && jest --watch",
    "test:cov": "cross-env NODE_ENV=test && jest --coverage",
    "test:debug": "cross-env NODE_ENV=test && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test && jest --config ./test/jest-e2e.json",
    "prisma:migrate:reset:dev": "dotenv -e .env.development -- npx prisma migrate reset",
    "prisma:migrate:reset:force": "dotenv -e .env.development -- npx prisma migrate reset --force",
    "prisma:migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "prisma:studio": "dotenv -e .env.development -- npx prisma studio",
    "prisma:generate": "prisma generate",
    "db:push:dev": "dotenv -e .env.development -- npx prisma db push",
    "db:push:test": "dotenv -e .env.test -- npx prisma db push",
    "db:push:prod": "dotenv -e .env.production -- npx prisma db push",
    "db:seed:dev": "dotenv -e .env.development -- ts-node prisma/seed.ts",
    "db:seed:test": "dotenv -e .env.test -- ts-node prisma/seed.ts",
    "db:seed:prod": "dotenv -e .env.production -- ts-node prisma/seed.ts",
    "test:users:dev": "cross-env NODE_ENV=development ts-node scripts/test_base_users_module.ts",
    "test:users:test": "cross-env NODE_ENV=test ts-node scripts/test_base_users_module.ts",
    "test:users:prod": "cross-env NODE_ENV=production ts-node scripts/test_base_users_module.ts",
    "tw:dev": "tailwindcss -i ./src/styles/tailwind.css -o ./public/css/tailwind.css --watch --verbose",
    "tw:build": "tailwindcss -i ./src/styles/tailwind.css -o ./public/css/tailwind.css --minify",
    "tw:build:dev": "tailwindcss -i ./src/styles/tailwind.css -o ./public/css/tailwind.css",
    "tw:clean": "rm -f ./public/css/tailwind.css",
    "tw:purge": "tailwindcss -i ./src/styles/tailwind.css -o ./public/css/tailwind.css --content 'src/**/*.{ts,js,html,hbs}' 'src/docs/templates/**/*.{hbs,ts}' --purge",
    "dev:full": "concurrently \"npm run start:dev\" \"npm run tw:dev\"",
    "build:full": "npm run tw:build && npm run build",
    "build:full:dev": "npm run tw:build:dev && npm run build",
    "postinstall": "prisma generate",
    "prepare": "husky"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@prisma/client": "^6.15.0",
    "@tailwindcss/cli": "^4.1.13",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "cron": "^4.3.0",
    "dayjs": "^1.11.13",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.1.5",
    "marked": "^16.2.1",
    "nodemailer": "^6.10.1",
    "prisma": "^6.15.0",
    "reflect-metadata": "^0.2.2",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "rxjs": "^7.8.1",
    "signale": "^1.4.0",
    "tailwindcss": "^4.1.13",
    "unified": "^11.0.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^7.0.1",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
